                function executePHP(url){
                    $.ajax({
                        url: url,
                        type: 'POST',
                        data: {
                            'action': 'count_up',
                            'message_id': 5                        },
                        success: (content) => {
                            console.log('php success');
                        }
                    });
                }
            });
